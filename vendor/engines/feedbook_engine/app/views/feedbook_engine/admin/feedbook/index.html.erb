<% content_for :user_details do %>
  <%= render partial: 'shared/user_details' %>
<% end %>
<% content_for :actions do %>
  <%= render partial: 'shared/actions' %>
<% end %>

<div class="admin-list">
  <header>
    <h1>Manage Technical Quizes</h1>

    <%= form_tag('#', method: "get", id: "search-form") do %>
      <p>Search</p>
      <%= text_field_tag :search, params[:search], class: "form-control" %>
      <%= button_tag "Search", name: nil do %>
        <div class="search-glass"></div>
      <% end %>
    <% end %>

    <%= link_to '< Back', '#', class: "orange hidden" %>
    <%= link_to 'List quizes', admin_quizes_path %>
    <%= link_to 'List questions', admin_questions_path %>
  </header>


  <table class="table">
    <tbody>
      <tr>
        <th>Badge &nbsp; &nbsp;</th>
        <th>Description</th>
        <th>Award</th>
        <th>Value</th>
        <th>Actions</th>
      </tr>

    </tbody>
  </table>
</div>

<%= render 'shared/admin_footer' %>
